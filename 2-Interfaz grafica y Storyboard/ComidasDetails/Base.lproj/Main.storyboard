                        <subviews>
                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" textAlignment="natural" translatesAutoresizingMaskIntoConstraints="NO" id="bP1-64-9p7">
                                <rect key="frame" x="0.0" y="64" width="600" height="536"/>
